{
  "name": "plantuml",
  "displayName": "PlantUML",
  "description": "Preview & generate PlantUML diagrams in VSCode.",
  "icon": "images/icon.png",
  "version": "1.0.5",
  "publisher": "jebbs",
  "engines": {
    "vscode": "^1.5.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/qjebbs/vscode-plantuml/issues"
  },
  "homepage": "https://github.com/qjebbs/vscode-plantuml/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/qjebbs/vscode-plantuml.git"
  },
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:diagram",
    "onCommand:plantuml.export",
    "onCommand:plantuml.exportAll",
    "onCommand:plantuml.preview"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "diagram",
        "extensions": [
          ".wsd",
          ".pu",
          ".puml"
        ],
        "aliases": [
          "Diagram",
          "PlantUML"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "diagram",
        "scopeName": "source.wsd",
        "path": "./syntaxes/diagram.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "plantuml.export",
        "title": "%plantuml.export.title%",
        "category": "PlantUML"
      },
      {
        "command": "plantuml.exportAll",
        "title": "%plantuml.exportAll.title%",
        "category": "PlantUML"
      },
      {
        "command": "plantuml.preview",
        "title": "%plantuml.preview.title%",
        "category": "PlantUML"
      }
    ],
    "keybindings": [
      {
        "command": "plantuml.preview",
        "key": "Alt+d"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "PlantUML configuration",
      "properties": {
        "plantuml.exportFormat": {
          "type": "string",
          "default": "",
          "enum": [
            "", "png", "svg", "eps", "pdf", "vdx", "xmi", "scxml", "html", "txt", "utxt", "latex", "latex:nopreamble"
          ],
          "description": "export format. Leave it blank to pick format everytime you export."
        },
        "plantuml.exportSubFolder": {
          "type": "boolean",
          "default": true,
          "description": "export files to a folder which has same name with current file."
        },
        "plantuml.exportConcurrency": {
          "type": "number",
          "default": 3,
          "description": "decides concurrency count when export multiple diagrams."
        },
        "plantuml.autoUpdatePreview": {
          "type": "boolean",
          "default": true,
          "description": "decides if automatically update the preview window."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
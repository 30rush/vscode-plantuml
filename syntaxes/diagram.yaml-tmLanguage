  Comment:
    patterns:
    - comment: comment line
      name: comment.line.comment.source.wsd
      begin: (?i)^\s*(')
      end: (?i)\n
    - comment: comment block
      name: comment.block.source.wsd
      begin: (?i)^\s*(/')
      end: (?i)^.*('/)
  Style:
    patterns:
